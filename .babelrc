{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@src": "./src"
        },
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ]
  ]
}